<project>
    <id>multi-module-overloaded</id>
    <name>Multi module overloading test</name>

    <!-- OpenL project includes one or more modules.  -->
    <modules>

        <module>
            <name>MODULE</name>
            <type>api</type>
            <!--
                Rules root document. Usually excel file on file system.
            -->
            <rules-root path="project*/Module*.xlsx"/>

        </module>
        <module>
            <name>RuntimeContextRouter</name>
            <rules-root path="RuntimeContextRouter.xlsx"/>
        </module>
    </modules>
</project>